  <dt><strong><%= model_class.human_attribute_name(:from_time) %>:</strong></dt>
  <dd><%= @booking.from_time %></dd>
  <dt><strong><%= model_class.human_attribute_name(:to_date) %>:</strong></dt>
  <dd><%= @booking.to_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:to_time) %>:</strong></dt>
  <dd><%= @booking.to_time %></dd>
  <dt><strong><%= model_class.human_attribute_name(:repeats) %>:</strong></dt>
