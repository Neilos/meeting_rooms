  <dt><strong><%= model_class.human_attribute_name(:room_id) %>:</strong></dt>
  <dd><%= @booking.room_id %></dd>
  <dt><strong>Booked by:</strong></dt>
  <dd><%= @booking.booker.name %></dd>
  <dt><strong>Booked created:</strong></dt>
  <dd><%= @booking.created_at%></dd>
  <dt><strong>Booked last updated:</strong></dt>
  <dd><%= @booking.updated_at%></dd>
