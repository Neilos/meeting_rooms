<%- model_class = User -%>
<% if @users.length > 0 %>
  <div class="page-header">
    <p>Select the new member from the list of users below.</p>
  </div>
  <table class="table table-hover" id="users-table">
    <thead>
      <tr>
        <th>Users</th>
      </tr>
    </thead>
    <tbody id="search_results_table_body">
      <% @users.each do |user| %>
        <tr id=<%= user.id %> class="user_results_row">
          <td><%= user.name %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% else %>
  <div class="page-header">
    <p>No matching results</p>
  </div>
<% end %>
